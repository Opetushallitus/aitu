{{=[[ ]]=}}
[[#i18n.arviointipaatokset]]

<h1>[[arviointipaatokset]]</h1>

<!-- Excel-latauksen palautelogin näkyminen -->
<div ng-show="naytalatauspalaute">
  <div>Excelin sisäänluku suoritettu, tulos</div>
  <div ng-repeat="teks in latauslogiteksti track by $index">{{teks}}</div>
  <hr/>
  <div><button ng-click="piilotapalaute()">Piilota latausloki</button></div>
  <hr/>
</div>


<div ng-show="!naytalatauspalaute">
<div>
  [[excel_lataus]]
  <fieldset>
    <file-upload api-metodi="{{'[[base-url]]/api/suoritus/excel-lataus'}}" upload-valmis="uploadValmis"></file-upload>
  </fieldset>
  <a href="{{'[[base-url]]/api/suoritus/suoritusraportti?edelliset-kayttaja=true'}}">Edellisen latauksen tiedot (ed. viisi minuuttia)</a>
</div>

<div>
  [[excel_luonti]]
  <br/>
  <a href="{{'[[base-url]]/api/suoritus/excel-luonti?kieli=fi'}}" target="_blank">Tiedosto suomeksi.</a><br/>
  <a href="{{'[[base-url]]/api/suoritus/excel-luonti?kieli=sv'}}" target="_blank">Tiedosto ruotsiksi.</a>
</div>

<section class="suoritukset">
  <h2>[[suoritukset]]</h2>
  <button class="button-with-icon add-icon-white" ng-click="lisaaSuoritus()">[[lisaa_suoritus]]</button>

  <suoritukset></suoritukset>
</section>

<section class="suorittajat">
  <suorittajat></suorittajat>
</section>

<br/>
</div>
      
[[/i18n.arviointipaatokset]]
